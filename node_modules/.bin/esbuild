#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/esbuild/bin/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/esbuild/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules:/home/thadeu/Qodeless/www/nodejs/node_modules:/home/thadeu/Qodeless/www/node_modules:/home/thadeu/Qodeless/node_modules:/home/thadeu/node_modules:/home/node_modules:/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/esbuild/bin/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/esbuild/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules:/home/thadeu/Qodeless/www/nodejs/node_modules:/home/thadeu/Qodeless/www/node_modules:/home/thadeu/Qodeless/node_modules:/home/thadeu/node_modules:/home/node_modules:/node_modules:/home/thadeu/Qodeless/www/nodejs/02-api-rest-nodejs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
